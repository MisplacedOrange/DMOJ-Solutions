#include <bits/stdc++.h>
#include <iostream>
using namespace std;

int main() {

    int n,m;

    cin >> n >> m;

    vector<string> a(n);
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    vector<string> noun(m);
    for (int i = 0; i < m; i++) {
            cin >> noun[i];
        }
    for (int i = 0; i < m; i++){
        for (int p = 0; p < n; p++)
            {cout << a[p] << " as " << noun[i] << endl;
        }
    }
}
